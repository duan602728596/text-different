    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-changed": "^3.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
